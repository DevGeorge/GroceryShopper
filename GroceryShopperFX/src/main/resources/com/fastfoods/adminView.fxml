<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fastfoods.AdminViewController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label alignment="CENTER" prefHeight="57.0" prefWidth="600.0" text="Admin Panel">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label alignment="CENTER" prefHeight="31.0" prefWidth="600.0" text="AUTHORIZED USERS ONLY" textFill="RED">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <GridPane alignment="CENTER" hgap="10.0" prefHeight="315.0" prefWidth="600.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="customerViewButton" mnemonicParsing="false" onAction="#customerViewButtonOnAction" prefHeight="133.0" prefWidth="300.0" text="Customer View! " />
                  <Button fx:id="accountEditorButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#accountEditorOnAction" prefHeight="133.0" prefWidth="300.0" text="Account Editor" textAlignment="CENTER" GridPane.rowIndex="1" />
                  <Button fx:id="itemEditorButton" mnemonicParsing="false" onAction="#itemEditorButtonOnAction" prefHeight="133.0" prefWidth="300.0" text="Item Editor" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="viewOrdersButton" alignment="CENTER" mnemonicParsing="false" onAction="#viewOrdersButtonOnAction" prefHeight="133.0" prefWidth="300.0" text="View Orders" GridPane.columnIndex="1">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin></Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </GridPane>
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="133.0" prefWidth="300.0" text="Close" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
